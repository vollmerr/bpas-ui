{
  "name": "bpas-ui",
  "version": "0.1.5",
  "main": "./lib/index.js",
  "private": true,
  "engines": {
    "node": ">=6.6.0"
  },
  "files": [
    "lib"
  ],
  "dependencies": {
    "bootstrap": "3",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router-dom": "^4.1.1",
    "styled-components": "^2.1.1"
  },
  "devDependencies": {
    "@storybook/addon-info": "^3.1.6",
    "@storybook/addon-knobs": "^3.1.8",
    "@storybook/addon-options": "^3.1.6",
    "@storybook/react": "^3.1.8",
    "autoprefixer": "7.1.1",
    "babel-cli": "^6.24.1",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-preset-react-app": "^3.0.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cpx": "^1.5.0",
    "cross-env": "^5.0.1",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "3.19.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.1.0",
    "extract-text-webpack-plugin": "2.1.2",
    "file-loader": "0.11.2",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "npm-run-all": "^4.0.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "react-bootstrap": "^0.31.1",
    "react-dev-utils": "^3.0.2",
    "react-error-overlay": "^1.0.9",
    "react-test-renderer": "^15.6.1",
    "storybook-router": "^0.2.8",
    "style-loader": "0.18.2",
    "sw-precache-webpack-plugin": "0.11.3",
    "url-loader": "0.5.9",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.5.0",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "npm run storybook",
    "prebuild": "rimraf lib",
    "build": "npm-run-all --parallel build:commonjs build:assets build:copy-files",
    "build:assets": "cpx \"./src/components/**/*.{css,eot,svg,ttf,woff,png}\" ./lib",
    "build:copy-files": "node scripts/copyBuildFiles.js",
    "build:commonjs": "cross-env NODE_ENV=production babel ./src/components --out-dir ./lib --ignore test.js",
    "test": "node scripts/test.js --env=jsdom",
    "storybook": "start-storybook -p 6006",
    "predeploy": "npm run build",
    "deploy": "cd lib && npm publish && cd ../"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
